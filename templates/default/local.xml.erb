<config>
  <global>
    <install>
      <date><![CDATA[<%= @inst_date %>]]></date>
    </install>
    <crypt>
      <key><![CDATA[<%= @enc_key %>]]></key>
    </crypt>
    <disable_local_modules>false</disable_local_modules>
    <resources>
      <db>
        <table_prefix><![CDATA[<%= @db_prefix %>]]></table_prefix>
      </db>
      <default_setup>
        <connection>
          <host><![CDATA[<%= @db_host %>:<%= @db_port %>]]></host>
          <username><![CDATA[<%= @db_user %>]]></username>
          <password><![CDATA[<%= @db_pass %>]]></password>
          <dbname><![CDATA[<%= @db_name %>]]></dbname>
          <initStatements><![CDATA[<%= @db_init %>]]></initStatements>
          <model><![CDATA[<%= @db_model %>]]></model>
          <type><![CDATA[<%= @db_type %>]]></type>
          <pdoType><![CDATA[<%= @db_pdo %>]]></pdoType>
          <active><%= @db_active %></active>
        </connection>
      </default_setup>
    </resources>
    <session_save><![CDATA[<%= @session[:save] %>]]></session_save>
    <% if @session[:save_path] %>
    <session_save_path><![CDATA[<%= @session[:save_path] %>]]></session_save_path>
    <% end %>
    <cache>
      <backend>memcached</backend>
      <slow_backend>database</slow_backend>
      <slow_backend_store_data><![CDATA[0]]></slow_backend_store_data>
      <auto_refresh_fast_cache><![CDATA[0]]></auto_refresh_fast_cache>
      <memcached>
        <servers>
          <server>
            <host><![CDATA[<%= @cache_info['servers'] %>]]></host>
            <port><![CDATA[<%= @cache_info['server_port'] %>]]></port>
            <persistent><![CDATA[<%= @cache_info['persistent'] %>]]></persistent>
            <weight><![CDATA[<%= @cache_info['weight'] %>]]></weight>
            <timeout><![CDATA[<%= @cache_info['timeout'] %>]]></timeout>
            <retry_interval><![CDATA[<%= @cache_info['retry_interval'] %>]]></retry_interval>
          </server>
        </servers>
        <compression><![CDATA[<%= @cache_info['compression'] %>]]></compression>
        <cache_dir><![CDATA[]]></cache_dir>
        <hashed_directory_level><![CDATA[]]></hashed_directory_level>
        <hashed_directory_umask><![CDATA[]]></hashed_directory_umask>
        <file_name_prefix><![CDATA[<%= @db_prefix %>]]></file_name_prefix>
      </memcached>
    </cache>
  </global>
  <admin>
    <routers>
      <adminhtml>
        <args>
          <frontName><![CDATA[<%= @admin_path %>]]></frontName>
        </args>
      </adminhtml>
    </routers>
  </admin>
</config>
